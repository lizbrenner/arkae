{
  "name": "@arkae/ai-tools",
  "version": "0.1.0",
  "description": "AI-powered utilities for Arkae",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@arkae/tokens": "workspace:*",
    "@arkae/primitives": "workspace:*",
    "@anthropic-ai/sdk": "^0.20.0",
    "openai": "^4.28.4"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.4.0"
  },
  "keywords": ["ai", "arkae", "llm"],
  "license": "MIT"
}
