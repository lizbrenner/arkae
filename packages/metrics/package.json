{
  "name": "@arkae/metrics",
  "version": "0.1.2",
  "description": "Usage tracking for Arkae design system",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@arkae/primitives": "workspace:*",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.4.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "keywords": [
    "metrics",
    "analytics",
    "arkae"
  ],
  "license": "MIT"
}
